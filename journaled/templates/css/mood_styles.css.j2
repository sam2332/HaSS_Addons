/* Generated Mood Styles */

{% for category, moods in mood_colors.items() %}
    /* {{ category.capitalize() }} Moods */
    {% for mood, styles in moods.items() %}
.mood_{{ mood }} {
    background-color: {{ styles.background }};
    color: {{ styles.color }};
}
.wc_mood_{{ mood }} {
    color: {{ styles.background }};
}
    {% endfor %}
{% endfor %}
